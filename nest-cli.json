{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": ["@nestjs/swagger"],
    "assets": [{ "include": "migrations/**/*", "outDir": "dist" }],
    "tsConfigPath": "tsconfig.json"
  }
}
